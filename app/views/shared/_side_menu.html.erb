  <section class="col-sm-4" style="background-color: white;">
  
    <div style="margin-left: 50px">
    <h2>Tipo</h2>
    <%@recipe_types.each do |type|%>
      <%=link_to type.name, recipe_type_path(type.id)%><br>
    <%end%>
    </div>

    <div style="margin-left: 50px">
    <h2>Cozinha</h2>
    <%@cuisines.each do |cuisine|%>
      <article>
       <%=link_to cuisine.name, cuisine_path(cuisine.id)%><br>
      </article>
    <%end%>
    </div>

  </section>