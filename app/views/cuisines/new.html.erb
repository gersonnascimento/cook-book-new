<div style="margin-top: 25px">
  <div class="row">
    <%=render 'shared/side_menu'%>

    <section class="col-sm-5 text-center">
      <%if flash[:error] %>
        <div class="alert alert-danger text-center" role="alert"><%= flash[:error] %></div>
      <% end %>
      <% @cuisine.errors.full_messages.each do |msg| %>
        <p><%= msg%><p>
      <%end%>
      <div>
        <%=form_for @cuisine do |f|%>
          <%=f.label :name%>
          <%=f.text_field :name%>
          <%=f.submit 'Enviar'%>
        <%end%>
      </div>
    </section>

    <section class="col-sm-5 text-center">
      <section>
        <h1>Favoritas</h1> <!-- nada por enquanto-->
      </section>
    </section>


  </div>
</div>



